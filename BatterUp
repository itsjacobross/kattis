#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int n, math, counter = 0;
    double sum = 0.0;
    scanf("%d",&n);
    for(int i=0; i<n; i++){
        scanf("%d",&math);
        if(math<0){continue;}
        counter++;
        sum += math;
    }
    printf("%.20lf\n", sum/counter);
    return 0;
}

// Code taken from https://github.com/JonSteinn/Kattis-Solutions/blob/master/src/Batter%20Up/C/main.c 
// Thanks to JonSteinn I can work without strings.

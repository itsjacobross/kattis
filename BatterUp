#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>

int main()
{
    int n; scanf("%d",&n);
    char bases[301]; int c;
    scanf("%c",bases); gets(bases);
    double num = 0; double denom = 0;
    for(c = 0; bases[c] != '\0'; ++c){
    printf("%c\n",bases[c]);
    if('1' == bases[c]){num=num+1; denom=denom+1;}
    else if('2' == bases[c]){num=num+2; denom=denom+1;}
    else if('3' == bases[c]){num=num+3; denom=denom+1;}
    else if('4' == bases[c]){num=num+4; denom=denom+1;}
    else if('0' == bases[c]){num=num; denom=denom+1;}
    else{num=num; denom=denom;}
    printf("Numerator: %lf\n",num);
    printf("Denominator: %lf\n",denom);
    }
    double slug = num/denom;
    printf("%0.20lf",slug);
    return 0;
}

